eyframes shake {\r\n    0% {\r\n      transform: translate(30px);\r\n    }\r\n\r\n    20% {\r\n      transform: translate(-30px);\r\n    }\r\n\r\n    40% {\r\n      transform: translate(15px);\r\n    }\r\n\r\n    60% {\r\n      transform: translate(-15px);\r\n    }\r\n\r\n    80% {\r\n      transform: translate(8px);\r\n    }\r\n\r\n    100% {\r\n      transform: translate(0px);\r\n    }\r\n  }\r\n\r\n  .input {\r\n    width: 1000px;\r\n    margin: 0 auto;\r\n    margin-top: 2rem;\r\n    max-width: 90%;\r\n    padding: 1rem;\r\n  }\r\n .inputData {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    flex-direction: row;\r\n  } \r\n\r\n  .inputData input {\r\n    margin-left: 2rem;\r\n    outline: none;\r\n    border: none;\r\n    background: transparent;\r\n    font-family: 'Poppins', Arial, Helvetica, sans-serif;\r\n    font-size: 1rem;\r\n    letter-spacing: .1rem;\r\n    color: rgb(42, 131, 47);\r\n    font-weight: 600;\r\n    width: 60%;\r\n    border-bottom: 1.3px solid rgb(146, 149, 151);\r\n    transition: .25s;\r\n    padding-bottom: 9px;\r\n  }\r\n  .inputData input::placeholder {\r\n    color: rgb(146, 149, 151);\r\n    letter-spacing: 0rem;\r\n    font-weight: normal;\r\n}\r\n\r\n.inputData input:is(:focus) {\r\n    border-bottom: 2px solid rgb(42, 131, 47);\r\n  }\r\n\r\n  .enterBtn {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n  }\r\n\r\n  .btn {\r\n    border: none;\r\n    font-family: 'Lato', Arial, Helvetica, sans-serif;\r\n    font-weight: 600;\r\n    font-size: .9em;\r\n    margin: 30px 10px;\r\n    background: transparent;\r\n    color: rgb(42, 131, 47);\r\n    padding: 7px 25px;\r\n    border: 2px solid;\r\n    text-transform: uppercase;\r\n    border-radius: 7px;\r\n    letter-spacing: .09rem;\r\n    cursor: pointer;\r\n  }\r\n\r\n  .canvas-container {\r\n    width: 1000px;\r\n    margin: 0 auto;\r\n    max-width: 90%;\r\n    max-height: 800px;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n  }\r\n\r\n  .btn:hover {\r\n    background: rgb(42, 131, 47);\r\n    color: #fff;\r\n    transition: .25s;\r\n  }\r\n\r\n  .result {\r\n    display: none;\r\n    align-items: center;\r\n    justify-content: center;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .reset {\r\n    display: none;\r\n  }\r\n\r\n  #suggest {\r\n    color: #000;\r\n    font-family: 'Poppins', Arial, Helvetica, sans-serif;\r\n    font-size: 1rem;\r\n    text-align: center;\r\n    font-weight: 600;\r\n    margin: 1.3rem;\r\n  }\r\n  \r\n  /* responsiveness */\r\n  @media screen and (max-width: 720px) {\r\ninput {\r\n      min-width: 20.3rem;\r\n    }\r\n    nav .container h1{\r\n      font-size: 1.9rem;\r\n    }\r\n  }\r\n  \r\n  @media screen and (max-width: 546px) {\r\n.inputData {\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      flex-direction: column;\r\n    }\r\n    .inputData h3{\r\n    margin-bottom: 1.3rem;\r\n    }\r\n    \r\n  }\r\n  "},"lineCount":226}},"error":null,"hash":"5ddac1cdac0f32a6b012a7ef2e8e58c4","cacheData":{"env":{}}}